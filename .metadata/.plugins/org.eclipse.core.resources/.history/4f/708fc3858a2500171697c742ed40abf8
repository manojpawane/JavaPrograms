	class Employee1 {
		private int id;
		private String name;
		private double salary;
		static  int hashcode =0;
		
		public Employee1(int id, String name, double salary) {
			this.id = id;
			this.name = name;
			this.salary = salary;
		}

		public int getId() {
			return id;
		}

		public void setId(int id) {
			this.id = id;
		}

		public String getName() {
			return name;
		}

		public void setName(String name) {
			this.name = name;
		}
	 
		@Override
		public int hashCode() {
			return hashcode++;
		}

		/*@Override
		public boolean equals(Object obj) {
			Employee1 emp = (Employee1) obj;
			return emp.getName().compareTo(this.getName())==0 &&
			emp.getId() == this.getId() && emp.getSalary()==this.getSalary();
		}*/
		public double getSalary() {
			return salary;
		}

		public void setSalary(double salary) {
			this.salary = salary;
		}

		@Override
		public String toString() {
			return "Employee1 [id=" + id + ", name=" + name + ", salary=" + salary + "]";
		}

	}


