import java.util.*;
public class FrogJump 
{
	
	public static void main(String[] args)
	{
		int i=0;
		
		Scanner scanner=new Scanner(System.in);
		System.out.println("Enter the Height of Well");
		int heightofwell=scanner.nextInt();
		
		int steps=frogjump(heightofwell);
	}
	
	public static void frogjump(int heightofwell)
	{
		int step=0;
		for(i=0;i<heightofwell;i=i+5)
		{
			i--;
			step++;
			
		}
		System.out.println("No. of Steps frog take to reach to its destination:"+step);
	}

}
