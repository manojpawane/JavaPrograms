
   package collections;

    import java.util.HashMap;
	import java.util.Iterator;
	import java.util.LinkedHashMap;
	import java.util.TreeSet;

	

	public class OverrideHashcode  
	{
		static int i=0;
		@Override
		public int hashCode() {
			return i++;
		}
		@Override
		public boolean equals(Object obj) {
			return false;
		
		}
		public static void main(String[] args) {
			java.util.LinkedHashSet lstr = new java.util.LinkedHashSet<>();
			HashMap<String, String> hashMap=new HashMap<>();
			
		
			
			Employee1 emp = new Employee1(10, "Manoj", 50000);
			
			lstr.add(emp);
			lstr.add(emp);
			
			System.out.println(lstr);
			System.out.println(lstr.size());
			
			
			TreeSet treeSet = new TreeSet<>();
			treeSet.add("Has");
			
			
		}
		
	}

	class Employee1 {
		private int id;
		private String name;
		private double salary;
		static  int hashcode =0;
		
		public Employee1(int id, String name, double salary) {
			this.id = id;
			this.name = name;
			this.salary = salary;
		}

		public int getId() {
			return id;
		}

		public void setId(int id) {
			this.id = id;
		}

		public String getName() {
			return name;
		}

		public void setName(String name) {
			this.name = name;
		}
	 
		@Override
		public int hashCode() {
			return hashcode++;
		}

		/*@Override
		public boolean equals(Object obj) {
			Employee1 emp = (Employee1) obj;
			return emp.getName().compareTo(this.getName())==0 &&
			emp.getId() == this.getId() && emp.getSalary()==this.getSalary();
		}*/
		public double getSalary() {
			return salary;
		}

		public void setSalary(double salary) {
			this.salary = salary;
		}

		@Override
		public String toString() {
			return "Employee1 [id=" + id + ", name=" + name + ", salary=" + salary + "]";
		}

	}




	