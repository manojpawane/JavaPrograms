import java.io.File;
import java.util.Scanner;

public class BinaryWord
{
	
private Scanner x;
public void openFile(){
	try{
		x=new Scanner(new File("text.txt"));
	}
	catch(Exception e){
		System.out.println("Could not find file");
	}
}
public void BinaryWord(){
	
	while(x.hasNext())
	{
		for(int i=0;i<x.hasNext();i++)
		{
			String [] arr=x.next();
			
		}
		
	}
	public void closeFile(){
		x.close();
	}
}



}
